{
  "kind": "collectionType",
  "collectionName": "blog_post_tags",
  "info": {
    "singularName": "blog-post-tag",
    "pluralName": "blog-post-tags",
    "displayName": "Blog Post Tag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "text": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "text",
      "required": true
    },
    "blog_posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "tags"
    }
  }
}
