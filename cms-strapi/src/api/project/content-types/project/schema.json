{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "product_url": {
      "type": "string",
      "unique": true
    },
    "repository_url": {
      "type": "string",
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "related_projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project"
    },
    "related_blog_posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-post.blog-post"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "release_date": {
      "type": "date"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project-tag.project-tag",
      "inversedBy": "projects"
    }
  }
}
