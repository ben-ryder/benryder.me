{
  "kind": "collectionType",
  "collectionName": "project_tags",
  "info": {
    "singularName": "project-tag",
    "pluralName": "project-tags",
    "displayName": "Project Tag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "text": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "text",
      "required": true
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "tags"
    }
  }
}
